{
  "name": "ashchan/auth-accounts",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.2",
    "hyperf/command": "^3.1",
    "hyperf/config": "^3.1",
    "hyperf/database": "^3.1",
    "hyperf/database-pgsql": "^3.1",
    "hyperf/db-connection": "^3.1",
    "hyperf/di": "^3.1",
    "hyperf/engine": "^2.0||^3.0",
    "hyperf/framework": "^3.1",
    "hyperf/http-server": "^3.1",
    "hyperf/logger": "^3.1",
    "hyperf/redis": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "dg/bypass-finals": "^1.9",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "start": "php bin/hyperf.php start"
  }
}
